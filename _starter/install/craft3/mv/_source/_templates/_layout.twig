{# Allow only visitors that are logged in or visitors visiting from whitelisted IP addresses #}
{% do checkAccessByIp() %}

<!DOCTYPE html>

{% minify %}
  <html class="no-js" lang="en">
    <head>
<%- include('_ejs/_head_meta') %>

<%- include('_ejs/_head_scripts') %>

<%- include('_ejs/_head_tracking') %>
      {% block critcss %}<!-- Crit CSS -->{% endblock %}
    </head>
    <body>
      {% minify %}
      <div id="page">
        {% block header %}
          {% include 'partials/_ui' %}
        {% endblock %}

        <section class="page__wrapper">
          {% block contentBefore %}{% endblock %}
          {% block content %}{% endblock %}
          {% block contentAfter %}{% endblock %}
        </section>

        {% block footer %}
          {% include 'partials/_footer' %}
        {% endblock %}
      </div>
      {% endminify %}

      {{ getAdminBarAssets() }}

      {% block jsFooter %}
<%- include('_ejs/_body_load_js') %>
      {% endblock %}
    </body>
  </html>
{% endminify %}