<table class="styles__example__table">
    <thead>
    <td><b>Name</b></td>
    <td><b>Type</b></td>
    <td><b>Default</b></td>
    <td><b>Description</b></td>
    </thead>
    <tbody>
    <% options.forEach((item) => { %>
    <tr>
        <td><b><%= item.name %></b><%= item.required ? ' (required)' : '' %></td>
        <td><%= item.type %></td>
        <td><i><%= item.default || 'null' %></i></td>
        <td><%= item.description %></td>
    </tr>
    <% }) %>
    </tbody>
</table>